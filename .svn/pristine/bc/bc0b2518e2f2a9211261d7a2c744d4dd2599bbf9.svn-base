	if(NULL != _starupBtn)
		_starupBtn->setVisible(false);
	if(NULL != _inlayBtn)
		_inlayBtn->setVisible(false);
	if(NULL != _strengthenBtn)
		_strengthenBtn->setVisible(false);
	if(NULL != _levelupBtn)
		_levelupBtn->setVisible(false);
	if(NULL != _maichuBtn)
		_maichuBtn->setVisible(false);
