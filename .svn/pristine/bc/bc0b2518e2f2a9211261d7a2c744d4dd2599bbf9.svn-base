			short PosIndex = (short)equipTypeData.posIndex + 1;
			selfEquip = dynamic_cast<f_equipment*>(PackageManager::getInstance()->getItem(EICT_Equip, PosIndex));
		}
		if (selfEquip != NULL && isShowSelfEquip)
