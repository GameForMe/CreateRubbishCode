void EquipTipUiLayer::GetDecomBagListOK()
{		
	if(NULL != _desEquip)
	{
		NET_SIC_Decompose_begin send;
		send.decomposeid[0] = _desEquip->get_item_id();
		//TCP_CLIENT->send_net_cmd(&send, NP_NORMAL, false);
