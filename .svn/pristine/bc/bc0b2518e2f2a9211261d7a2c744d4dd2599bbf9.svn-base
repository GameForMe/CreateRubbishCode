	}
	else if (WCT_PACKAGEUI == showType)
	{
		strNormal = "rubeibaobtnn";
		strSelected = "rubeibaobtnh";
	}
	else if(WCT_TradingUI == showType || WCT_ForgingDecomUI == showType)
